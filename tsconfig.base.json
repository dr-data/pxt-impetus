{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5", "dom", "dom.iterable"
    ],
    "module": "commonjs",

    "baseUrl": ".",
    "rootDir": ".",

    "newLine": "lf",

    "sourceMap": false,

    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "removeComments": true,
    "strict": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
    